</head>
<body>
	
	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100 p-t-85 p-b-20">
				<form class="login100-form validate-form" action = /login method="post">
					<span class="login100-form-title p-b-70">
						Welcome
					</span>
                    <div>
                        <h3 class ="errorh3"><%=error%></h3>
                    </div>
					<div class="wrap-input100 validate-input m-t-85 m-b-35" data-validate = "Enter username">
						<input class="input100" type="text" name="username">
						<span class="focus-input100" data-placeholder="Username"></span>
					</div>

					<div class="wrap-input100 validate-input m-b-50" data-validate="Enter password">
						<input class="input100" type="password" name="pass">
						<span class="focus-input100" data-placeholder="Password"></span>
					</div>

					<div class="container-login100-form-btn">
						<button class="login100-form-btn">
							Login
						</button>
					</div>

					<ul class="login-more p-t-190">
						<li class="m-b-8">
							<span class="txt1">
								Forgot
							</span>

							<a href="/ForgotPassword" class="txt2">
								Username / Password?
							</a>
						</li>

						<li>
							<span class="txt1">
								Donâ€™t have an account?
							</span>

							<a href="/Signup" class="txt2">
								Sign up
							</a>
						</li>
					</ul>
				</form>
			</div>
		</div>
	</div>
	
    
    <style>
        h3.errorh3
        {
            color: red;
        }
    </style>
    
	<div id="dropDownSelect1"></div>
<!--===============================================================================================-->
	<script src="dist/LogInFiles/vendor/jquery/jquery-3.2.1.min.js"></script>
<!--===============================================================================================-->
	<script src="dist/LogInFiles/vendor/animsition/js/animsition.min.js"></script>
<!--===============================================================================================-->
	<script src="dist/LogInFiles/vendor/bootstrap/js/popper.js"></script>
	<script src="dist/LogInFiles/vendor/bootstrap/js/bootstrap.min.js"></script>
